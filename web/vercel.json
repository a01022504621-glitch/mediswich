{
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    { "source": "/index.html", "destination": "/", "permanent": true },
    { "source": "/:path((?!.*\\.).+)\\.html", "destination": "/:path", "permanent": true },
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "mediswich.co.kr" }],
      "destination": "https://www.mediswich.co.kr/:path*",
      "permanent": true
    }
  ]
}
